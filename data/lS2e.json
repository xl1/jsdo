\n\n/* animations */\n@mixin animations($list...) {\n  $fillmode: ();\n  @each $_ in $list {\n    $fillmode: append($fillmode, forwards, comma);\n  }\n  -webkit-animation: $list;\n  -webkit-animation-fill-mode: $fillmode;\n  animation: $list;\n  animation-fill-mode: $fillmode;\n}\n$move: move ($tick * $length) linear;\n@-webkit-keyframes move {\n  from { -webkit-transform: translateY(-$length * $height); }\n  to   { -webkit-transform: translateY(0); }\n}\n@keyframes move {\n  from {\n    -webkit-transform: translateY(-$length * $height);\n    transform: translateY(-$length * $height);\n  }\n  to {\n    -webkit-transform: translateY(0);\n    transform: translateY(0);\n  }\n}\n@-webkit-keyframes switch {\n  0% { width: 0; }\n  50%, 100% { width: $width; }\n}\n@keyframes switch {\n  0% { width: 0; }\n  50%, 100% { width: $width; }\n}\n@mixin cover { width: 100vw; height: 100vh; }\n\n\n/* gimmick */\nbody {\n  margin: 0;\n  overflow: hidden;\n}\n.grid {\n  float: left;\n  width: $width;\n  height: $height;\n  @include animations($move);\n}\n.right {\n  margin-left: -$width;\n  @include animations(\n    $move, switch $tick steps(1, end) (-0.5 * $tick) infinite\n  );\n}\n.item {\n  transition: width 999s 9999s;\n  &:hover {\n    transition: none;\n    width: 0;\n  }\n}\n\n.cover:hover, .asteroid:hover ~ .cover {\n  @include cover;\n}\n@for $i from 1 through $da-max {\n  .left.da#{$i}:hover ~ .cover {\n    @include animations(explosion-left#{$i} 1ms linear ($i * $tick));\n  }\n  .right.da#{$i}:hover ~ .cover {\n    @include animations(explosion-right#{$i} 1ms ($i * $tick));\n  }\n  @-webkit-keyframes explosion-left#{$i}  { from, to { @include cover; }}\n  @-webkit-keyframes explosion-right#{$i} { from, to { @include cover; }}\n  @keyframes explosion-left#{$i}  { from, to { @include cover; }}\n  @keyframes explosion-right#{$i} { from, to { @include cover; }}\n}\n\n\n/* design */\n.main {\n  margin: 0 auto;\n  width: 5 * $width;\n  height: 100vh;\n  background-color: #003;\n  color: #fff;\n  &:not(:hover) * {\n    -webkit-animation-play-state: paused;\n    animation-play-state: paused;\n  }\n  a { color: #fff; }\n}\n.grid, .goal {\n  cursor: $cursor 15 15, move;\n}\n.item {\n  margin-left: -$width;\n  background-image: $item;\n  background-repeat: no-repeat;\n  background-size: 75%;\n  background-position: 50%;\n}\n.asteroid {\n  background-image: $asteroid;\n  background-size: 100%;\n}\n.goal {\n  @include animations($move);\n  height: 5 * $height;\n  background-image: $earth;\n  background-repeat: no-repeat;\n  background-size: 5 * $width 2 * $width;\n  background-clip: border-box;\n}\n.cover {\n  position: absolute;\n  left: 0;\n  top: 0;\n  width: 0;\n  height: 0;\n  overflow: hidden;\n  background: #a00;\n}\n.goal, .cover {\n  text-align: center;\n  line-height: 50vh;\n  font-size: large;\n}"},"published":"2013-09-05T23:07:51"}