M: 15;\n$SENSORLENGTH: 10;\n$LENGTH: 45;\n\n$tick: 400ms;\n$size: 50px;\n\n\n// shorthands for convenience\n$move: move ($LENGTH * $tick) linear;\n@mixin explode {\n  width: 100vw;\n  height: 100vh;\n  background: yellow;\n}\n\n// animations\n%explode-now {\n  -webkit-animation: explosion 1ms;\n  animation: explosion 1ms;\n  -webkit-animation-fill-mode: forwards;\n  animation-fill-mode: forwards;\n}\n@-webkit-keyframes explosion { from, to { @include explode; }}\n@keyframes explosion { from, to { @include explode; }}\n\n@-webkit-keyframes move {\n  from { -webkit-transform: translate(0, 0); }\n  to   { -webkit-transform: translate(0, $LENGTH * $size); }\n}\n@keyframes move {\n  from { transform: translate(0, 0); }\n  to   { transform: translate(0, $LENGTH * $size); }\n}\n@-webkit-keyframes switch {\n  0% { width: 0; }\n  50%, 100% { width: 33vw; }\n}\n@keyframes switch {\n  0% { width: 0; }\n  50%, 100% { width: 33vw; }\n}\n\n\nbody {\n  margin: 0;\n  overflow: hidden;\n}\n\n// sensors\n.sensor {\n  width: 33vw;\n  height: $size;\n  position: absolute;\n  &:hover ~ .cover {\n    background: yellow;\n  }\n}\n.sensor-left {\n  -webkit-animation: $move, switch $tick steps(1, end) infinite;\n  animation: $move, switch $tick steps(1, end) infinite;\n  -webkit-animation-fill-mode: forwards;\n  animation-fill-mode: forwards;\n}\n.sensor-right {\n  -webkit-animation: $move, switch $tick steps(1, end) (-0.5 * $tick) infinite;\n  animation: $move, switch $tick steps(1, end) (-0.5 * $tick) infinite;\n  -webkit-animation-fill-mode: forwards;\n  animation-fill-mode: forwards;\n} \n.sensor0 {\n  -webkit-animation: $move;\n  animation: $move;\n  -webkit-animation-fill-mode: forwards;\n  animation-fill-mode: forwards;\n  background: black;\n  &:hover ~ .cover {\n    @extend %explode-now;\n  }\n}\n@for $i from 1 through $SENSORLENGTH {\n  .sensor#{$i} {\n    margin-top: $i * 50px;\n    background: mix(white, red, $i * 10%);\n    \n    &.sensor-left:hover ~ .cover {\n      -webkit-animation: explosion-left#{$i} 1ms ($i * $tick);\n      -webkit-animation-fill-mode: forwards;\n      animation: explosion-left#{$i} 1ms ($i * $tick);\n      animation-fill-mode: forwards;\n    }\n    &.sensor-right:hover ~ .cover {\n      -webkit-animation: explosion-right#{$i} 1ms ($i * $tick);\n      -webkit-animation-fill-mode: forwards;\n      animation: explosion-right#{$i} 1ms ($i * $tick);\n      animation-fill-mode: forwards;\n    }\n  }\n  @-webkit-keyframes explosion-left#{$i} { from, to { @include explode; }}\n  @keyframes explosion-left#{$i} { from, to { @include explode; }}\n  @-webkit-keyframes explosion-right#{$i} { from, to { @include explode; }}\n  @keyframes explosion-right#{$i} { from, to { @include explode; }}\n}\n\n// game end\n.cover {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 0;\n  height: 0;\n  overflow: hidden;\n  background: yellow;\n  &:hover {\n    @extend %explode-now;\n  }\n}\n.goal {\n  -webkit-animation: $move;\n  animation: $move;\n  -webkit-animation-fill-mode: forwards;\n  animation-fill-mode: forwards;\n  position: absolute;\n  left: 0;\n  top: -$LENGTH * $size;\n}\n\n.sensor:hover {\n  background: cyan !important;\n}\n\n\n// positioning\n$tops: 2, 2, 4, 8, 10, 13, 15, 20, 22, 22, 26, 29, 31, 33, 35;\n@for $i from 1 through $LINENUM {\n  .line#{$LINENUM - $i} {\n    left: ($i % 3) * 33vw;\n    top: nth($tops, $i) * -$size;\n  }\n}\n"},"published":"2013-08-20T00:54:46"}